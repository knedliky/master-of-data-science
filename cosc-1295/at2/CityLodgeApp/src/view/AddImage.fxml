<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="225.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AddImageController">
   <children>
      <VBox alignment="CENTER" fillWidth="false" nodeOrientation="LEFT_TO_RIGHT" prefHeight="220.0" prefWidth="400.0">
         <children>
            <HBox alignment="CENTER_LEFT" fillHeight="false" prefHeight="20.0" prefWidth="400.0" spacing="15.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Melbourne City Lodge">
                     <font>
                        <Font name="Futura Medium" size="11.0" />
                     </font>
                  </Text>
                  <Region prefHeight="0.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
                  <Text fx:id="date" strokeType="OUTSIDE" strokeWidth="0.0" text="date">
                     <font>
                        <Font name="Andale Mono" size="10.0" />
                     </font>
                  </Text>
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </HBox>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Adding Image">
               <font>
                  <Font name="Futura Medium" size="16.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="15.0" />
               </VBox.margin>
            </Text>
            <VBox alignment="CENTER_LEFT" prefHeight="246.0" prefWidth="347.0" spacing="20.0">
               <children>
                  <HBox alignment="CENTER" prefHeight="50.0" prefWidth="347.0" spacing="20.0">
                     <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Room ID:" />
                        <TextField fx:id="roomId" prefHeight="29.0" prefWidth="44.0" promptText="123">
                           <font>
                              <Font size="11.0" />
                           </font></TextField>
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" layoutX="10.0" layoutY="32.0" prefHeight="50.0" prefWidth="347.0" spacing="10.0">
                     <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Image URL:" />
                        <TextField fx:id="imageURL" prefHeight="4.0" prefWidth="157.0" promptText="/user/img/pathway.jpg">
                           <font>
                              <Font size="11.0" />
                           </font>
                        </TextField>
                        <Button mnemonicParsing="false" onMouseClicked="#handleMouseEvent" text="Search">
                           <font>
                              <Font name="System Italic" size="9.0" />
                           </font>
                        </Button>
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" prefHeight="27.0" prefWidth="228.0" spacing="15.0">
                     <children>
                        <Button fx:id="OK" alignment="BOTTOM_LEFT" contentDisplay="CENTER" mnemonicParsing="false" text="OK" />
                        <Button fx:id="cancel" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" text="Cancel" />
                     </children>
                  </HBox>
               </children>
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </VBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
